<!-- Main component for a primary marketing message or call to action -->
<div class="panel panel-default">
	<div class="panel-heading">Please login / signup</div>
	<div class="panel-body">
		<form class="form-horizontal">
			<div class="form-group">
				<label class="col-sm-2 control-label">Phone Number</label>
				<div class="col-sm-10">
					<input class="form-control" id="inputContactName" placeholder=""/>
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-2 control-label" for="inputUserPassword">Password</label>
				<div class="col-sm-10">
					<input type="password" class="form-control" id="inputUserPassword" placeholder="">
				</div>
			</div>
			<button type="submit" class="btn btn-primary col-sm-offset-2 col-sm-10">Login / Signup</button>
		</form>
		
	</div>
</div>